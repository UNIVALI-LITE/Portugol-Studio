programa 
{
	funcao inicio(cadeia args[])
	{
		real num1, num2, num3, soma, raiz

		escreva("Digite Três números reais\n")
		leia(num1,num2,num3)
		
		soma = num1+num2+num3
		raiz = soma/2

		escreva("A soma dos numeros  ", soma, "\n")
		escreva("A raiz quadrada da soma dos produtos  ", raiz)
	}
}
